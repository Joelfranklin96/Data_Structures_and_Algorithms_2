   "source": [
    "#Find sum of an array using Recursion"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "1c100a87",
   "metadata": {},
   "outputs": [],
   "source": [
    "def sumArray(arr):\n",
    "    \n",
    "    if len(arr) == 1:\n",
    "        return arr[0]\n",
    "    else:\n",
    "        return arr[0] + sumArray(arr[1:])"
   ]
  },
