   "source": [
    "#Calculate a to the power of b using Recursion"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "id": "37fb1277",
   "metadata": {},
   "outputs": [],
   "source": [
    "def power(a,b):\n",
    "    if b == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return a*power(a,b-1)"
   ]
  },
