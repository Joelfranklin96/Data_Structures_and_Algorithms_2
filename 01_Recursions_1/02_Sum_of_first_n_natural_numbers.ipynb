   "source": [
    "#Calculate the sum of first n natural numbers using Recursion"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "ed942a23",
   "metadata": {},
   "outputs": [],
   "source": [
    "def sum(n):\n",
    "    \n",
    "    if n == 0:\n",
    "        return 0\n",
    "    else:\n",
    "        return n + sum(n-1)"
   ]
