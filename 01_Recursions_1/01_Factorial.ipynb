   "source": [
    "#Calculate the Factorial of a number using Recursion"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "cfaa7650",
   "metadata": {},
   "outputs": [],
   "source": [
    "def factorial(n):\n",
    "    \n",
    "    if n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * factorial(n-1)  "
   ]
  },
